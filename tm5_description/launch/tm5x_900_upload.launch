<?xml version="1.0"?>
<launch>
  <!-- <arg name="limited" default="false" doc="If true, limits joint range [-PI, PI] on all joints." /> -->

  <arg name="stl_mesh" default="false" />
  <arg name="kinematics_config" default="$(find tm5_description)/config/tm5x_900_default.yaml" />

  <arg name="transmission_hw_interface" default="hardware_interface/PositionJointInterface" />

  <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find tm5_description)/urdf/tm5x_900_robot.urdf.xacro'
    stl_mesh:=$(arg stl_mesh) transmission_hw_interface:=$(arg transmission_hw_interface) kinematics_config:=$(arg kinematics_config)" />
</launch>
